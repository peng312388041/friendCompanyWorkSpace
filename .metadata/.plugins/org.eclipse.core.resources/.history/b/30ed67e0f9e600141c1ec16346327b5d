package excel;

import java.util.List;

import jxl.write.WritableSheet;

public class BaseSheet {
	String sheetId; // 保存sheet id
	String sheetName; // 保存sheet 显示的名称
	int sheetType; // 大分类0，中分类1，小分类2，商品类3

	BaseSheet parent;

	WritableSheet sheetEntity;

	public WritableSheet getSheetEntity() {
		return sheetEntity;
	}

	public void setSheetEntity(WritableSheet sheetEntity) {
		this.sheetEntity = sheetEntity;
	}

	List<String> header;

	public String getSheetId() {
		return sheetId;
	}

	public void setSheetId(String sheetId) {
		this.sheetId = sheetId;
	}

	public String getSheetName() {
		return sheetName;
	}

	public void setSheetName(String sheetName) {
		this.sheetName = sheetName;
	}

	public int getSheetType() {
		return sheetType;
	}

	public void setSheetType(int sheetType) {
		this.sheetType = sheetType;
	}

	public List<String> getHeader() {
		return header;
	}

	public void setHeader(List<String> header) {
		this.header = header;
	}

	public BaseSheet getParent() {
		return parent;
	}

	public void setParent(BaseSheet parent) {
		this.parent = parent;
	}

}
